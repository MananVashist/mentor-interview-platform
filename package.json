{
  "name": "mentor-interview-platform",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && node scripts/copy-public-files.js && node scripts/generate-static-seo.js"
  },
  "dependencies": {
    "@expo-google-fonts/dancing-script": "^0.4.2",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^14.0.4",
