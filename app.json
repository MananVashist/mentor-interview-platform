{
  "expo": {
    "name": "CrackJobs",
    "slug": "crackjobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#11998e"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ecaacc3d-80b3-4cd6-b148-87ab80b8246f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crackjobs.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "CFBundleDisplayName": "CrackJobs",
        "CFBundleName": "CrackJobs",
        "NSCameraUsageDescription": "CrackJobs needs camera access for video interviews with mentors.",
        "NSMicrophoneUsageDescription": "CrackJobs needs microphone access for audio during video interviews."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#11998e"
      },
      "package": "com.crackjobs.app",
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "blockedPermissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-font",
      "expo-asset",
      "@react-native-community/datetimepicker"
    ],
    "scheme": "crackjobs",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ecaacc3d-80b3-4cd6-b148-87ab80b8246f"
      }
    },
    "owner": "mananvashist",
    "description": "Practice mock interviews with verified expert mentors from Google, Amazon, Meta and top companies. Get comprehensive feedback, improve your interview skills, and land your dream job."
  }
}
