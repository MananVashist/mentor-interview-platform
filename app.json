{
  "expo": {
    "name": "CrackJobs",
    "slug": "crackjobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#11998e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crackjobs.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "CFBundleDisplayName": "CrackJobs",
        "CFBundleName": "CrackJobs"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#11998e"
      },
      "package": "com.crackjobs.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "CrackJobs - Ace Your Next Interview",
      "shortName": "CrackJobs",
      "description": "Master your interview skills with expert mentors. Practice mock interviews across PM, SDE, HR roles, get personalized feedback, and land your dream job. 3-round structured interview preparation platform.",
      "themeColor": "#11998e",
      "backgroundColor": "#ffffff",
      "lang": "en",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait-primary",
      "categories": ["education", "business", "productivity"],
      "screenshots": [],
      "related_applications": [],
      "prefer_related_applications": false,
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "default",
        "apple-mobile-web-app-title": "CrackJobs",
        "application-name": "CrackJobs",
        "msapplication-TileColor": "#11998e",
        "msapplication-tap-highlight": "no"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "expo-font"
    ],
    "scheme": "crackjobs",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b4bb97e0-d7da-494a-86e0-04f5f41faebe"
      }
    },
    "owner": "mananvashist",
    "description": "Master your interview skills with expert mentors. Practice mock interviews across different roles (PM, SDE, HR, Data Science), get detailed feedback after each round, and crack your dream job. Structured 3-round interview preparation with personalized mentorship.",
    "primaryColor": "#11998e",
    "keywords": [
      "interview preparation",
      "mock interviews",
      "career coaching",
      "job interview practice",
      "mentor platform",
      "interview feedback",
      "career development",
      "job search",
      "technical interviews",
      "behavioral interviews",
      "PM interviews",
      "SDE interviews",
      "HR interviews",
      "data science interviews",
      "interview skills",
      "job coaching"
    ]
  }
}